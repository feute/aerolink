{
  "name": "aerolink",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync && husky install",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-auto": "^1.0.0-next.49",
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.23",
    "@sveltejs/kit": "^1.0.0-next.346",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts,cjs,svelte}": "eslint --cache --fix",
    "*.{js,ts,cjs,svelte,json,html,css,md}": "prettier --write"
  },
  "dependencies": {
    "dayjs": "^1.11.0",
    "firebase": "^9.6.10",
    "firebase-admin": "^10.0.2",
    "svelte-flatpickr": "^3.2.6",
    "svelte-markdown": "^0.2.2"
  }
}
